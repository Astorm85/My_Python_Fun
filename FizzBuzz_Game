<!doctype html>
<html lang="en">
<meta charset="utf-8">
<title>FizzBuzz</title>
<pre id="out"></pre>
<script>
  const out=[]; 
  for (let i=1;i<=100;i++){
    if(i%15===0) out.push("FizzBuzz");
    else if(i%3===0) out.push("Fizz");
    else if(i%5===0) out.push("Buzz");
    else out.push(String(i));
  }
  document.getElementById("out").textContent = out.join("\n");
</script>
</html>
